		enable-gpios = <&gpio0 RK_PC7 GPIO_ACTIVE_HIGH>;
		reset-gpios = <&gpio0 RK_PD5 GPIO_ACTIVE_LOW>;
	//	pinctrl-names = "default";
	//	pinctrl-0 = <&lcd0_rst_gpio>;
	};
};

&dsi0_in_vp0 {
	status = "okay";
};

&dsi0_in_vp1 {
	status = "disabled";
